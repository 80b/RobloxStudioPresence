				<ProtectedString name="Source"><![CDATA[local httpService = game:GetService("HttpService")
local url = "http://galaxybotl.altervista.org/data.php"

local scripts = {}
local oldLen = {}

local locations = {
	game.Workspace,
	game.Players,
	game.Lighting,
	game.ReplicatedFirst,
	game.ReplicatedStorage,
	game.ServerScriptService,
	game.ServerStorage,
	game.StarterGui,
	game.StarterPack,
	game.SoundService,
	game.Chat
}

function send(data)
	data = httpService:JSONEncode(data)
	return httpService:PostAsync(url,data)
end

local userid = nil

local button = plugin:CreateToolbar("Roblox Studio Presence"):CreateButton("Start","Start","http://www.roblox.com/asset/?id=2780048880")

userid = nil

local gui = script.RobloxStudioPresence:Clone()
if not game.CoreGui:FindFirstChild("RobloxStudioPresence") then
	gui.Parent = game.CoreGui
	gui.Enabled = false
end

button.Click:Connect(function()
	gui.Enabled = not gui.Enabled
end)

gui.Main.Start.MouseButton1Click:Connect(function()
	local success, message = pcall(function()
		local username = gui.Main.Username.Text
		local uid = game.Players:GetUserIdFromNameAsync(username)
		if uid then
			userid = uid
			gui.Enabled = false
		end
	end)
	if not success then warn(message) end
end)

repeat wait() until userid ~= nil

plugin.Unloading:Connect(function()
	send({
		["action"] = "REMOVE",
		["data"] = {
			["userid"] = userid
		}
	})
end)

function getIndex(tab,obj)
	for i = 1, #tab do
		if tab[i] == obj then
			return i
		end
	end
end

function remove(tab,ind)
	if tab[ind] then
		table.remove(tab,ind)
	end
end

function getScripts(obj)
	for i = 1, #obj:GetChildren() do
		local child = obj:GetChildren()[i]
		if child then
			if child:IsA("Script") or child:IsA("LocalScript") or child:IsA("ModuleScript") then
				scripts[#scripts+1] = child
			end
			child.ChildAdded:Connect(function(ch)
				if ch:IsA("Script") or ch:IsA("LocalScript") or ch:IsA("ModuleScript") then
					scripts[#scripts+1] = ch
					oldLen[ch] = #ch.Source
				end
				getScripts(ch)
			end)
			child.ChildRemoved:Connect(function(ch)
				if ch:IsA("Script") or ch:IsA("LocalScript") or ch:IsA("ModuleScript") then
					local ind = getIndex(scripts,ch)
					remove(scripts,ind)
				end
			end)
			getScripts(child)
		end
	end
end

for i = 1, #locations do
	local loc = locations[i]
	loc.ChildAdded:Connect(function(ch)
		if ch:IsA("Script") or ch:IsA("LocalScript") or ch:IsA("ModuleScript") then
			scripts[#scripts+1] = ch
			oldLen[ch] = #ch.Source
		end
		getScripts(ch)
	end)
	loc.ChildRemoved:Connect(function(ch)
		if ch:IsA("Script") or ch:IsA("LocalScript") or ch:IsA("ModuleScript") then
			local ind = getIndex(scripts,ch)
			remove(scripts,ind)
		end
	end)
end

for i = 1, #locations do
	loc = locations[i]
	if loc then
		getScripts(loc)
	end
end

for i = 1, #scripts do
	local scr = scripts[i]
	if scr then
		oldLen[scr] = #scr.Source
	end
end

while true do
	for i = 1, #scripts do
		local scr = scripts[i]
		if scr then
			local old = oldLen[scr]
			if #scr.Source ~= old then
				oldLen[scr] = #scr.Source
				local data = {
					["action"] = "SET",
					["data"] = {
						["userid"] = userid,
						["game"] = game.Name,
						["scriptName"] = scr.Name,
						["isEditing"] = "true",
						["path"] = "no",
					}
				}
				send(data)
			end
		end
	end
	wait(1)
end

]]></ProtectedString>
